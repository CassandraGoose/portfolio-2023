---
export interface Props {
  title: string;
  tech: string;
}

const { title, tech } = Astro.props;
---

<li class="my-16">
  <p class='font-bold my-2' data-cy="project-details-title">
    {title}
  </p>
  <hr class='border-black' />
  <p class='italic flex my-2 text-lg'>{tech}</p>
  <slot />
  <li></li>
</li>
