---
import Card from "../components/Card.astro";
const { title, date, link } = Astro.props;

---

<li class="ml-4 mb-4">
  <a
    class="italic text-stone-500 hover:text-stone-600"
    href={link}
    target="_blank"
    rel="noopener noreferrer"
    >{title}</a
  >
  <p class="text-lg">{date}</p>
</li>
